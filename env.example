# Google OAuth Credentials
# Get these from https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your_client_id_here
GOOGLE_CLIENT_SECRET=your_client_secret_here

# NextAuth Configuration
# Generate a secret with `openssl rand -base64 32` or similar
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# Tockify Configuration
NEXT_PUBLIC_TOCKIFY_WIDGET_ID=your_tockify_widget_id_here

# Database (PostgreSQL)
DATABASE_URL="postgresql://user:password@localhost:5432/gentogen?schema=public"

# Iyzico Payment
IYZICO_API_KEY=your_iyzico_api_key
IYZICO_SECRET_KEY=your_iyzico_secret_key
IYZICO_BASE_URL=https://sandbox-api.iyzipay.com
